{
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    },
    "rules": {
        "jsx-quotes": [2, "prefer-double"],
        "no-trailing-spaces": 0,
        "eco-list": 0,
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "no-alert": 0,
        "no-lone-blocks": 0,
        "react/require-extension": "off"
    }
}